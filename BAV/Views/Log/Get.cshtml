
@{
    ViewData["Title"] = "Log";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12" style="min-height: auto !important;height: auto !important;">
            <!-- BEGIN EXAMPLE TABLE PORTLET-->
            <div class="portlet light" style="min-height: auto !important;height: auto !important">
                <div class="portlet-title">

                    <div class="caption col-md-12">
                        <span class="caption-subject font-green-sharp bold uppercase">
                            LOG
                        </span>
                    </div>
                    <div class="form-group col-md-3 pull-left">
                        <label class="text-center bold font-green-sharp" for="">Assembly Number</label>
                        <input id="txt-assembly-search" class="form-control" style="color: mediumvioletred" placeholder="Assembly Number" />
                    </div>

                    <div class="form-group col-md-3">
                        <label class="text-center bold font-green-sharp" for="">Date From</label>
                        <label class="control-label"></label>
                        <input type="datetime-local" id="txt-from-search" class="form-control" style="color: mediumvioletred" />
                    </div>
                    <div class="form-group col-md-3">
                        <label class="text-center bold font-green-sharp" for="">Date To</label>
                        <label class="control-label"></label>
                        <input type="datetime-local" id="txt-to-search" class="form-control" style="color: mediumvioletred" />
                    </div>

                    <div class="form-group col-md-12">
                        <a class="btn btn-success" id="btn-search">Search</a>
                        <a class="btn btn-success" id="btn-export">Export</a>

                        <a class="btn btn-success" id="btn-reset">Reset</a>
                    </div>
                </div>

                <div id="tbl-data" class="table-responsive-lg">

                </div>
                <div class="pagination" id="pagination">
                </div>

            </div>

        </div>

    </div>
</div>

@section Scripts {
    <script src="~/js/log.js"></script>
}
